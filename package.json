{
  "name": "@atstaeff/epf",
  "version": "1.0.1",
  "description": "Application for event processing in Azure",
  "main": "dist/app.js",
  "types": "dist/app.d.ts",
  "author": "STEFANPOSS",
  "contributors": [
    {
      "name": "Stefan Poss",
      "email": "sp@stefanposs.de"
    }
  ],
  "license": "MIT",
  "scripts": {
    "dev:test": "jest",
    "dev:doc": "typedoc --out docs lib/bin/app.ts",
